{
  "name": "scopedalerts",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "serve:ssr:scopedalerts": "node dist/scopedalerts/server/server.mjs"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "packageManager": "npm@10.9.2",
  "dependencies": {
    "@angular/cdk": "^21.0.0-rc.1",
    "@angular/common": "^21.0.0-next.0",
    "@angular/compiler": "^21.0.0-next.0",
    "@angular/core": "^21.0.0-next.0",
    "@angular/forms": "^21.0.0-next.0",
    "@angular/material": "^21.0.0-rc.1",
    "@angular/platform-browser": "^21.0.0-next.0",
    "@angular/platform-server": "^21.0.0-next.0",
    "@angular/router": "^21.0.0-next.0",
    "@angular/ssr": "^21.0.0-rc.1",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "firebase": "^11.0.1",
    "rss": "^1.2.2",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular/build": "^21.0.0-rc.1",
    "@angular/cli": "^21.0.0-rc.1",
    "@angular/compiler-cli": "^21.0.0-next.0",
    "@types/express": "^5.0.1",
    "@types/node": "^20.17.19",
    "jsdom": "^27.0.0",
    "typescript": "~5.9.2",
    "vitest": "^4.0.0"
  }
}
